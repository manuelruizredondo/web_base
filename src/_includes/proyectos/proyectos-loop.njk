 <section data-scroll data-scroll-offset="50%,50%" data-scroll-call="pageColor"   class="mbr-64 height-full-2  align-middle is-flex "
data-scroll-id="{{proyectos.data.id}} darkmode" id="{{proyectos.data.id}}">
  <div class="row sm:text-center w-100   ">
    <div class="col-xs-12 col-sm-6   ">
      <div   class="fade-right" >
          <div class="fluid-3-l numbers">
          {{proyectos.data.prename}}
        </div>
      </div>
      <div class="mb-16 ">
        <a href="{{ proyectos.url }}">
          <div  >
            <div  class="kv-block11-num">
              <div class="num"> 0{{proyectos.data.id}}</div>
            </div>
          </div>
          <div class=" mb-32">
            <p     class="fluid-100-b">{{proyectos.data.title}}
            </p>
          </div>
          <div class=" mbr-32">
            <p     class="fluid-24-l">{{proyectos.data.descripcion}}
            </p>
          </div>
          <div class=" mbr-32">
            <p     class="fluid-12-r"> Client: {{proyectos.data.descripcion}}
            </p>
          </div>
        </a>
        <div class="mb-24">
          <a    class="link-line"  href="{{ proyectos.url }}">Ver PROYECTO </a>
        </div>
      </div>
    </div>
    <div class="col-xs-12  col-sm-6  pb-40">
      <div class="rat-16-9">
               <div class="rat-content">
     <img  class="rellax video-cover"  data-rellax-percentage="0.5" data-rellax-speed="1"
         src="{{proyectos.data.image}} " alt="">       
                   </div>
             </div>
   </div>
  </div>
</section>

<article data-scroll data-scroll-id="{{proyectos.data.colors}} " id="{{proyectos.data.colors}}"
  data-scroll-call="pageColor" class=" height-full-2  align-middle is-flex " data-scroll-offset="50%,50%">
  <div data-scroll data-scroll-offset="55%,50%" data-scroll-class="opened" data-scroll-repeat="false"
    class="row w-100   ">
    <div class="col-xs-12 col-sm-3   ">
      <div data-scroll data-scroll-speed="2" class="front-section">
        <div class="fluid-3-l ">
          {{proyectos.data.prename}}
        </div>
        <div class="mb-16  front-section ">
    
       
            <div class="kv-block11-num">
              <div class="num"> 0{{ counter }} </div>
          </div>
          <div class="out-"><div class="in-">  <div class="fluid-3-b no-wrap text-capitalize">   {{proyectos.data.title}} </div> </div> </div>
          <div class="out-"><div class="in-">   <div class="fluid-8-l ">{{proyectos.data.descripcion}} </div> </div> </div>
          <div class="out- mb-64"><div class="in-">   <div class="fluid-10-r"> Client: {{proyectos.data.descripcion}} </div> </div> </div>


          <div class="out-"><div class="in-">   <a class="link-line c-accent " href="{{ proyectos.url }}">Ver proyecto </a>  </div> </div>

              
        </div>
    

      </div>
    </div>
    <div class="col-xs-12  col-sm-9  pb-40">
      <div data-scroll="" data-scroll-offset="50%,50%" data-scroll-speed="0.2" class="rat-16-9">
        <div class="rat-content">
          <div class="img-wrap">
            <div class="img-inner"
              style="background-image: url('/assets/static/proyectos/{{proyectos.data.imagemini}}');">
            </div>
          </div>
        </div>
      </div>
    </div>
  </div>
</article>